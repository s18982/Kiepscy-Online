@{
    ViewData["Title"] = "Episodes";
}
<h1>Lista odcinków</h1>
<table>
    <thead>
        <tr>
            <th>Odcinek</th>
            <th>Tytuł</th>            
            <th>Link</th>
        </tr>
    </thead>
    <tbody>
    @for (int i = 1; i < 577; i++)
    {
        <tr>
            <td> @ViewData["Data"+i+"_num"] </td>
            <td> @ViewData["Data" + i +"_title"] </td>
            <td>
                 <p><a href=@ViewData["Data" + i + "_end"]>klik.</a></p>
            </td>
        </tr>
    }

    </tbody>
</table>
